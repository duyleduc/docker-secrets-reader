{
    "compilerOptions": {
        "module": "ES2022",
        "esModuleInterop": true,
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "strictNullChecks": false,
        "strictFunctionTypes": false,
        "strictPropertyInitialization": false,
        "noImplicitAny": false,
        "strict": false,
        "declaration": true
    },
    "lib": [
        "es2022"
    ],
    "include": ["src"]
}
