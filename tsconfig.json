{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "strictNullChecks": false,
        "strictFunctionTypes": false,
        "strictPropertyInitialization": false,
        "noImplicitAny": false,
        "strict": false
    },
    "lib": [
        "es2015"
    ],
    "include": ["src"]
}
